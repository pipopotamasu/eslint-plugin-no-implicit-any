    "@types/node": "^18.8.7",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/rule-tester": "^6.9.1",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "release-it": "^16.2.1",
    "ts-jest": "^29.1.1",
