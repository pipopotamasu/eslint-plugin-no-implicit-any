{
  "name": "eslint-plugin-no-implicit-any",
  "description": "eslint plugin to detect and format no implicit any",
  "version": "1.6.3",
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "keywords": [
    "eslint",
    "TypeScript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pipopotamasu/eslint-plugin-no-implicit-any"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "release": "npm run build && release-it",
    "eslint": "eslint \"rules/**/*.{ts,tsx}\"",
    "eslint:fix": "eslint \"rules/**/*.{ts,tsx}\" --fix",
    "prettier:write": "prettier --write .",
    "prettier:check": "prettier --check .",
    "fix": "npm run eslint:fix && npm run prettier:write"
  },
  "dependencies": {
    "@typescript-eslint/types": "^7.1.1",
    "@typescript-eslint/utils": "^7.1.1"
  },
  "peerDependencies": {
    "eslint": "^8.56.0",
    "typescript": "^5.0.0",
    "@typescript-eslint/parser": "^7.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/rule-tester": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "release-it": "^17.1.1",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5"
  }
}
